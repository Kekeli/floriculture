{
  "name": "floriculture",
  "main": "server.js",
  "version": "0.5.0",
  "private": true,
  "repository" : { 
    "type" : "git",
    "url" : "http://github.com/kekeli/floriculture.git"
  },
  "scripts": {
    "start": "node server.js",
    "lint": "./node_modules/.bin/jshint server.js ./config ./test ./app/models ./app/routes",
    "checkStyle": "./node_modules/.bin/jscs .",
    "pretest": "npm run-script lint && npm run-script checkStyle",
    "test": "node test/run",
    "coverage": "./node_modules/.bin/istanbul cover ./node_modules/mocha/bin/_mocha --",
    "codeclimate": "./node_modules/.bin/istanbul cover ./node_modules/mocha/bin/_mocha --report lcovonly && cat ./coverage/lcov.info | ./node_modules/.bin/codeclimate && rm -rf ./coverage"
  },
  "dependencies": {
    "bcrypt-nodejs" : "latest",
    "body-parser": "^1.9.0",
    "connect-flash": "latest",
    "connect-mongo": "latest",
    "cookie-parser": "^1.3.0",
    "errorhandler": "^1.2.0",
    "express": "^4.0.0",
    "express-partials": "*",
    "express-session": "^1.9.0",
    "express-validator": "*",
    "ejs": "^1.0.0",
    "less-middleware": "*",
    "method-override": "^2.3.0",
    "mongoose": "^3.8.18",
    "morgan": "^1.5.0",
    "multer": "^0.1.6",
    "paginate": "*",
    "passport": ">= 0.0.0",
    "passport-local": "latest",
    "sendgrid": "latest",
    "serve-favicon": "~2.1.3"
  },
  "devDependencies": {
    "chai": "^1.9.2",
    "codeclimate-test-reporter": "0.0.4",
    "istanbul": "^0.3.2",
    "jscs": "^1.7.3",
    "jshint": "^2.5.6",
    "mocha": "^2.0.1",
    "nodemon": "^1.2.1",
    "rewire": "^2.1.0",
    "sinon": "^1.10.3",
    "supertest": "^0.15.0"
  },
  "keywords": [
    "node.js",
    "mongodb",
    "plants"
  ],
  "engines": {
    "node": "0.10.x"
  }
}
