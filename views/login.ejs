<form action="/login" method="post" accept-charset="utf-8">
	<div class="item-new">
		<label for="email">Email: </label>
			<input class="input" autocorrect="off" autocomplete="off" autocapitalize="off" type="text" name="email" />
		<label for="password">Password:</label> 
			<input class="input" type="password" name="password" placeholder="Enter Password" />
		
		<br />
		<input type="submit" class="btn btn-small btn-primary" value="Log On" id="btnSend" />
	</div>
</form>
